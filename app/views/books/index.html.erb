<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td>title</td>
        <td>より良いコードを書くためのシンプルで実践的なテクニック</td>
        <% @books.each do |book| %>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
        <td><a class="edit_26299" href="/books/26299/edit">Edit</a></td>
        <td><a data-confirm="Are you sure?" class="destroy_26299" rel="nofollow" data-method="delete" href="/books/26299">Destroy</a></td>
      </tr>
<% end %>
  </tbody>
</table>


<h2>New book</h2>
<%= form_with model:@book, local:true do |f| %>
<h8>タイトル</h8>
<%= f.text_field :title %>
<h8>本文</h8>
<%= f.text_area :body %>

<%= f.submit 'Create Book' %>

<% end %>